    "import hashlib\n",
    "\n",
    "text1 = \"hello\"\n",
    "text2 = \"hello!\"\n",
    "\n",
    "hash1 = hashlib.sha3_256(text1.encode()).hexdigest()\n",
    "hash2 = hashlib.sha3_256(text2.encode()).hexdigest()\n",
    "\n",
    "print(f\"Text1: {text1} => Hash: {hash1}\")\n",
    "print(f\"Text2: {text2} => Hash: {hash2}\")"
   ]
  }
